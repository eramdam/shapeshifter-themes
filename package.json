{
  "name": "glitch-twitterbot",
  "version": "0.0.1",
  "description": "Beep beep tweet!",
  "main": "server.js",
  "scripts": {
    "start": "tsx server/index.ts",
    "build": "tsc -p .",
    "typecheck": "tsc -p . --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "serve": "forever stop macthemes; forever --uid='macthemes' --append start dist/server/index.js"
  },
  "type": "module",
  "dependencies": {
    "@atproto/api": "^0.4.3",
    "@types/express": "^4.17.15",
    "@types/lodash": "^4.14.191",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^22.8.0",
    "@types/object-hash": "^3.0.2",
    "axios": "^1.2.1",
    "cheerio": "^1.0.0-rc.10",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "express": "^4.18.2",
    "forever": "^4.0.3",
    "gif-extract-frames": "^1.0.2",
    "husky": "^8.0.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "masto": "^5.0.4",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.0",
    "object-hash": "^3.0.0",
    "prettier": "^3.3.3",
    "semver": "^7.3.8",
    "sharp": "^0.32.6",
    "tsx": "^4.19.2",
    "twitter-api-v2": "^1.15.2",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "puppeteer": "^19.4.1"
  },
  "engines": {
    "node": "^22.10.0"
  },
  "repository": {
    "url": ""
