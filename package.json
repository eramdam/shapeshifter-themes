{
	"name": "glitch-twitterbot",
	"version": "0.0.1",
	"description": "Beep beep tweet!",
	"main": "server.js",
	"scripts": {
		"start": "ts-node server/index.ts",
		"build": "tsc -p .",
		"typecheck": "tsc -p . --noEmit",
